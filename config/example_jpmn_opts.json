{

  "modules": {

    "time-performance": {

      "debug-level": 4,

      "precision": 3,

      "time-modules": true,

      "display-full": true,

      "sort-func": "currentSideAvg"
    },


    "cache": {
      "clear-cache-button": false
    },


    "keybinds": {
      "toggle-hybrid-sentence": ["KeyN"],

      "toggle-highlight-word": ["KeyN"],

      "toggle-front-full-sentence-display": ["Quote"],

      "play-sentence-audio": ["KeyP"],

      "play-word-audio": ["KeyW"],

      "toggle-hint-display": ["Period"],

      "toggle-secondary-definitions-display": ["Digit8"],

      "toggle-additional-notes-display": ["Digit9"],

      "toggle-extra-definitions-display": ["Digit0"],

      "toggle-extra-info-display": ["BracketLeft"]
    },


    "sent-utils": {
      "enabled": true,

      "enable-display-sentence-processing": true,
      "enable-full-sentence-processing": true,

      "always-hide-full-sentence-front": false,

      "remove-final-period": true,
      "remove-final-period-on-altdisplay": false,

      "auto-highlight-word": {
        "enabled": true,

        "warn-on-auto-highlight": false,
        "warn-if-auto-highlight-fails": true
      },

      "pa-indicator-color-quotes": {
        "type": "viewport-width-is",
        "value": 1300,
        "greater": false,
        "lesser": true
      },

      "pa-sent-pa-indicator-color-quotes": {
        "type": "viewport-width-is",
        "value": 1300,
        "greater": false,
        "lesser": true
      },

      "auto-quote-sentence": true,

      "auto-quote-alt-display-sentence": true,

      "auto-quote-open": "「",
      "auto-quote-close": "」",

      "pa-sent-auto-quote-open": "『",
      "pa-sent-auto-quote-close": "』",

      "quote-match-strings": [
        ["「", "」"],
        ["『", "』"]
      ]
    },

    "vertical-display": {
      "enabled": true,
      "front-enabled": true,
      "back-enabled": true,
      "back-toggle-enabled": true    },

    "kanji-hover": {
      "enabled": true,

      "mode": 1,

      "load-delay": 300,

      "display-pitch-accent": true,

      "display-pitch-accent-hover-only": true,

      "click-word-to-browse": true,


      "non-new-query": "(-is:new OR (is:suspended is:new flag:3)) -(is:suspended flag:1)",

      "new-query": "is:new -(is:suspended (flag:1 OR flag:3))",

      "max-non-new-oldest": 2,
      "max-non-new-latest": 2,
      "max-new-latest": 2
    },


    "auto-pitch-accent": {
      "enabled": true,

      "reading-display-mode": 1,

      "search-for-ajt-word": true,

      "pa-positions": {

        "display-entire-dictionary": false,

        "default-connector": "・",

        "set-first-pitch-as-main": true
      },

      "pa-override": {

        "separators": ["・", "、"],
        "downstep-markers": ["＼"],
        "heiban-markers": ["￣"],

        "warn-on-invalid-format": true,

        "text-format-set-first-pitch-as-main": false,

        "heiban-marker-required": true,

        "kifuku-override": [-1]
      },

      "colored-pitch-accent": {
        "enabled": false,

        "color-reading": true,

        "color-display": true,

        "color-overline": true,

        "color-full-sentence": false,

        "color-definitions": false
      }
    },


    "img-utils": {
      "enabled": true,

      "resize-height-mode": "auto-height",
      "resize-height-fixed-value": 210,
      "stylize-images-primary-definition": {

        "enabled": true,

        "mode": {
          "yomichan": "collapse",

          "user": "collapse"
        },

        "mode-override-tags": {
          "none": ["img-no-styling", "jpmn-img-no-styling"],
          "collapse": ["img-collapse", "jpmn-img-collapse"],
          "float": ["img-float", "jpmn-img-float"],

          "yomichan-none": ["img-yomichan-no-styling", "jpmn-img-yomichan-no-styling"],
          "yomichan-collapse": ["img-yomichan-collapse", "jpmn-img-yomichan-collapse"],
          "yomichan-float": ["img-yomichan-float", "jpmn-img-yomichan-float"],

          "user-none": ["img-user-no-styling", "jpmn-img-user-no-styling"],
          "user-collapse": ["img-user-collapse", "jpmn-img-user-collapse"],
          "user-float": ["img-user-float", "jpmn-img-user-float"]
        }
      },

      "stylize-images-glossary": {
        "enabled": true
      },

      "primary-definition-picture": {

        "enabled": true,

        "position": "auto-bottom",




        "tags-bottom": ["img-bottom", "jpmn-img-buttom"],
        "tags-top": ["img-top", "jpmn-img-top"],
        "tags-right": ["img-right", "jpmn-img-right"]
      },

      "add-image-if-contains-tags": [


      ],

      "image-blur": {
        "enabled": false,

        "tags": ["NSFW", "nsfw", "-NSFW"],


        "toggle-states": {
          "type": "pc-mobile",
          "pc": [1, 2, 0],
          "mobile": [1, 2, 0]
        }
      }
    },


    "open-collapsed-fields": {
      "enabled": false,

      "open": [

      ],

      "open-on-new-enabled": true,

      "open-on-new": [

      ]

    },

    "collapse-dictionaries": {
      "enabled": false,

      "default-mode": {
        "secondary-definition": "collapsible-open",
        "extra-definitions": "collapsible-open"
      },

      "collapse-after-max-number": {
        "secondary-definition": 1,
        "extra-definitions": 1
      },

      "dictionary-options": {

        "always-collapsible-open": [],

        "always-collapsible-closed": [],

        "never-collapsible": ["JMdict (English)", "JMdict", "Nico/Pixiv"],

        "only-display-dictionary-name": []
      }

    },



    "word-indicators": {
      "enabled": true,

      "load-delay": 50,

      "display-pitch-accent": true,

      "display-pitch-accent-hover-only": false,

      "click-word-to-browse": true,

      "non-new-query": "(-is:new OR (is:suspended is:new flag:3)) -(is:suspended flag:1)",

      "new-query": "is:new -(is:suspended (flag:1 OR flag:3))",

      "max-non-new-oldest": 2,
      "max-non-new-latest": 2,
      "max-new-latest": 2

    },


    "info-circle-utils": {
      "enabled": true,

      "togglable-lock": {
        "enabled": true,

        "show-popup": {
          "type": "pc-mobile",
          "pc": true,
          "mobile": false
        }
      },

      "is-hoverable": {
        "type": "pc-mobile",
        "pc": true,
        "mobile": false
      }
    },

    "freq-utils": {
      "enabled": true,
      "max": 4,

      "overflow": 0
    },

    "fix-ruby-positioning": {
      "enabled": {
        "type": "pc-mobile",
        "pc": false,
        "mobile": false
      }
    },

    "check-duplicate-key": {
      "enabled": true
    }


  },


  "ignored-errors": [
    "ReferenceError: EFDRC is not defined."
  ],

  "hybrid-sentence-open-on-play-sentence": true,

  "click-card-sentence-reveal-on-back-side": true,

  "greyed-out-collapsible-fields-when-empty": false,

  "no-sentence-reading-warn": false,

  "filled-sentence-reading-with-empty-sentence-warn": true,

  "enable-ankiconnect-features": {
    "type": "pc-mobile",
    "pc": true,
    "mobile": false
  },

  "debug-to-console": false,

  "debug-level": 6
}
